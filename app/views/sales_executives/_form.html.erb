<%= form_for @sales_executive, :html => { :class => 'form-horizontal' } do |f| %>
  <div class="control-group">
    <%= f.label :employee_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.number_field :employee_id, :class => 'number_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :team_leader_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.number_field :team_leader_id, :class => 'number_field' %>
    </div>
  </div>


  <div class="form-actions">
    <%= f.submit nil, :class => 'btn btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                sales_executives_path, :class => 'btn' %>
  </div>
<% end %>
