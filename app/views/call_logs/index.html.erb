<%- model_class = CallLog -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:call_result) %></th>
      <th><%= model_class.human_attribute_name(:subject) %></th>
      <th><%= model_class.human_attribute_name(:call_type) %></th>
      <th><%= model_class.human_attribute_name(:call_purpose) %></th>
      <th><%= model_class.human_attribute_name(:call_start_time) %></th>
      <th><%= model_class.human_attribute_name(:call_duration) %></th>
      <th><%= model_class.human_attribute_name(:call_owner_id) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @call_logs.each do |call_log| %>
      <tr>
        <td><%= link_to call_log.id, call_log_path(call_log) %></td>
        <td><%= call_log.call_result %></td>
        <td><%= call_log.subject %></td>
        <td><%= call_log.call_type %></td>
        <td><%= call_log.call_purpose %></td>
        <td><%= call_log.call_start_time %></td>
        <td><%= call_log.call_duration %></td>
        <td><%= call_log.call_owner_id %></td>
        <td><%=l call_log.created_at %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_call_log_path(call_log), :class => 'btn btn-mini' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      call_log_path(call_log),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_call_log_path,
            :class => 'btn btn-primary' %>
