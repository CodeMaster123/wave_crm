<%= form_for @notification, :html => { :class => 'form-horizontal' } do |f| %>
  <div class="control-group">
    <%= f.label :contact_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :contact_id, :class => 'contacts_text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :body, :class => 'control-label' %>
    <div class="controls">
        <%= f.text_area :body, :rows => "7",  :class => 'text_area' %>
    </div>
  </div>

  <div class="control-group">
      <%= f.label :Notification_time, :class => 'control-label' %>
      <div class="controls">
          <%= f.text_field :notification_time, :class => 'datetime_select' %>
      </div>
  </div>

  <div class="form-actions">
      <%= f.submit nil, :class => 'btn btn-primary' %>
      <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
          notifications_path, :class => 'btn' %>
      </div>
  <% end %>
  <script>
      $(function() {
          var availableTags = [

          <% Contact.all.each do |c| %>
              <%= raw("\"#{c.name}\",") %>
          <% end %>

          ];
          $( ".contacts_text_field" ).autocomplete({
              source: availableTags
          });
      });
  </script>
