<%- model_class = TransactionFieldValue -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:transaction_id) %>:</strong></dt>
  <dd><%= @transaction_field_value.transaction_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:transaction_field_id) %>:</strong></dt>
  <dd><%= @transaction_field_value.transaction_field_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:transaction_field_value) %>:</strong></dt>
  <dd><%= @transaction_field_value.transaction_field_value %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              transaction_field_values_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_transaction_field_value_path(@transaction_field_value), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              transaction_field_value_path(@transaction_field_value),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
