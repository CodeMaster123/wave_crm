<%- model_class = TransactionFieldValue -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:transaction_id) %></th>
      <th><%= model_class.human_attribute_name(:transaction_field_id) %></th>
      <th><%= model_class.human_attribute_name(:transaction_field_value) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @transaction_field_values.each do |transaction_field_value| %>
      <tr>
        <td><%= link_to transaction_field_value.id, transaction_field_value_path(transaction_field_value) %></td>
        <td><%= transaction_field_value.transaction_id %></td>
        <td><%= transaction_field_value.transaction_field_id %></td>
        <td><%= transaction_field_value.transaction_field_value %></td>
        <td><%=l transaction_field_value.created_at %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_transaction_field_value_path(transaction_field_value), :class => 'btn btn-mini' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      transaction_field_value_path(transaction_field_value),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_transaction_field_value_path,
            :class => 'btn btn-primary' %>
